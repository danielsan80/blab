{% for year, months in yearMonths %}
    <strong>{{year}}</strong><br/>
    {% for month, monthName in months %}
        {% set yearMonth = year ~ '-' ~ month %}
        {% set params = params|merge({month:yearMonth}) %}
        <a href="{{path(route, params)}}" class="{{ yearMonth == current?'active':''}}" >{{monthName|lower}}</a> 
    {% endfor %}
{% endfor %}